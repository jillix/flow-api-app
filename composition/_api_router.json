                                "users.get",
                                {
                                    "emails": ["users.addEmail", {}],
                                    "projects": [
                                        "projects.create",
                                        {
                                            ":project": {
                                                "apps": ["apps.create", {}]
                                            }
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            },
            "PATCH": {
                "api": {
                    "users": {
                        ":username": [
                            "users.updateCore",
                            {
                                "password": "users.updatePassword",
                                "emails": {
                                    ":emailId": "users.updateEmail"
                                },
                                "projects": {
                                    ":project": [
                                        "projects.updateCore",
                                        {
                                            "apps": {
                                                ":app": ["apps.updateCore", {}]
                                            }
                                        }
                                    ]
                                }
                            }
                        ]
                    }
                }
            },
            "DELETE": {
                "api": {
                    "users": {
                        ":username": [
                            "users.delete",
                            {
                                "projects": {
                                    ":project": [
                                        "projects.delete",
                                        {
                                            "apps": {
                                                ":app": ["apps.delete", {}]
                                            }
                                        }
                                    ]
                                }
                            }
                        ]
                    }
                }
            }
        }
    },
