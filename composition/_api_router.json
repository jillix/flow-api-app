                        ":username": [
                            "users.get",
                            {
                                "projects": [
                                    "projects.list",
                                    {
                                        ":project": [
                                            "projects.get", 
                                            {
                                                "apps": [
                                                    "apps.list",
                                                    {
                                                        ":app": ["apps.get", {}]
                                                    }
                                                ]
                                            }
                                        ]
                                    }
                                ],
                                "apps": ["apps.listAll", {}]
                            }
                        ]
                    }
                }
            },
            "POST": {
                "api": {
                    "users": [
                        "users.create",
                        {
